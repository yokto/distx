{
        "env": {
        },
        "loop": {
                "ARCH": ["x86_64", "aarch64"],
                "FILE": ["main"]
        },
        "target": {
                "build": {
			"targets": ["{ARCH}-xbuild"]
		},
		"test": {
		},
		"{ARCH}-xbuild": {
			"exec": [[
				"{DISTX_DEP}/{DISTX_PREFIX}llvm-{DISTX_ARCH}/bin/clang++",
				"-target",
				"{ARCH}-unknown-linux-zwolf",
				"{DISTX_BUILD}/{ARCH}-{FILE}.o",
				"-o",
				"{DISTX_BUILD}/{ARCH}-xbuild"
			]],
			"targets": ["{ARCH}-{FILE}.o"]
		},
		"{ARCH}-{FILE}.o": {
			"exec": [[
				"{DISTX_DEP}/{DISTX_PREFIX}llvm-{DISTX_ARCH}/bin/clang++",
				"-target",
				"{ARCH}-unknown-linux-zwolf",
				"{FILE}.cpp",
				"-c",
				"-o",
				"{DISTX_BUILD}/{ARCH}-{FILE}.o"
			]]
		}
        }
}
